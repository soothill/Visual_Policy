{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3ServerAccessLogsPolicy",
      "Effect": "Allow",
      "Action": ["s3:PutObject"],
      "Resource": "arn:aws:s3:::your-logging-bucket/logs/*"
    },
    {
      "Sid": "AllowLoggingServiceToPutObjects",
      "Effect": "Allow",
      "Action": ["s3:GetBucketLogging", "s3:PutBucketLogging"],
      "Resource": "arn:aws:s3:::your-logging-bucket"
    }
  ]
}
